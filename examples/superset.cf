# Caffeine is a direct superset of Ansi C [C89]
/* Supports Ansi C comments */
// And C++ comments

import io from "std";

#include "stdio.h";

fun main[int](argc[int], argv[char **]) {
  log("Calling C code...");
  c_loop(10);
}

int i, j;
void c_loop(i) {
  for (j = 0; j <= i; j++) {
    printf("%d\n", i);
  }
}